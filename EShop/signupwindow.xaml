<Window x:Class="EShop.signupwindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EShop"
        mc:Ignorable="d"
        Title="signup" Height="250" Width="355">
    <Grid>
        <TextBox HorizontalAlignment="Left" Name="login" Margin="10,21,0,0" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top" Width="250"/>
        <TextBox HorizontalAlignment="Left" x:Name="password" Margin="10,44,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="250"/>
        <TextBox HorizontalAlignment="Left" x:Name="name" Margin="10,71,0,0" TextWrapping="Wrap" Text="name" VerticalAlignment="Top" Width="250"/>
        <TextBox HorizontalAlignment="Left" x:Name="adress" Margin="10,94,0,0" TextWrapping="Wrap" Text="adress" VerticalAlignment="Top" Width="250"/>
        <Button Content="Sign up" HorizontalAlignment="Left" Margin="10,132,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
